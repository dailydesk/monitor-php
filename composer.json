{
    "name": "dailydesk/monitor-php",
    "description": "DailyDesk Monitor for PHP applications.",
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "inspector-apm/inspector-php": "^3.8"
    },
    "autoload": {
        "psr-4": {
            "DailyDesk\\Monitor\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-main": "1.x-dev"
        }
    },
    "require-dev": {
        "pestphp/pest": "^2.36 | ^3.7",
        "symfony/var-dumper": "^6.0 | ^7.0"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
