{
    "name": "dailydesk/monitor-php",
    "description": "DailyDesk Monitor for PHP applications.",
    "license": "MIT",
    "require": {
        "php": "^8.0",
        "inspector-apm/inspector-php": "^3.8"
    },
    "autoload": {
        "psr-4": {
            "DailyDesk\\Monitor\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-main": "1.x-dev"
        }
    },
    "require-dev": {
        "pestphp/pest": "^3.7"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
