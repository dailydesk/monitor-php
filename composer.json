{
    "name": "monitor/php",
    "description": "Code Execution Monitoring for PHP",
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "inspector-apm/inspector-php": "^3.15"
    },
    "autoload": {
        "psr-4": {
            "Monitor\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-main": "1.x-dev"
        }
    },
    "require-dev": {
        "pestphp/pest": "^3.7",
        "symfony/var-dumper": "^7.2"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
